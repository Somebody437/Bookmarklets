javascript:(function()%20%7B%20%20%20%20%20function%20swapElements()%20%7B%20%20%20%20%20%20%20%20%20let%20elements%20=%20document.querySelectorAll(%22*%22);%20%20%20%20%20%20%20%20%20if%20(elements.length%20%3C%205)%20return;%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20let%20indices%20=%20new%20Set();%20%20%20%20%20%20%20%20%20while%20(indices.size%20%3C%205)%20%7B%20%20%20%20%20%20%20%20%20%20%20%20%20indices.add(Math.floor(Math.random()%20*%20elements.length));%20%20%20%20%20%20%20%20%20%7D%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20let%20selectedElements%20=%20Array.from(indices).map(i%20=%3E%20elements%5Bi%5D);%20%20%20%20%20%20%20%20%20let%20positions%20=%20selectedElements.map(el%20=%3E%20el.getBoundingClientRect());%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20for%20(let%20i%20=%200;%20i%20%3C%20selectedElements.length;%20i++)%20%7B%20%20%20%20%20%20%20%20%20%20%20%20%20let%20nextIndex%20=%20(i%20+%201)%20%25%20selectedElements.length;%20%20%20%20%20%20%20%20%20%20%20%20%20selectedElements%5Bi%5D.style.position%20=%20%22absolute%22;%20%20%20%20%20%20%20%20%20%20%20%20%20selectedElements%5Bi%5D.style.left%20=%20%60$%7Bpositions%5BnextIndex%5D.left%7Dpx%60;%20%20%20%20%20%20%20%20%20%20%20%20%20selectedElements%5Bi%5D.style.top%20=%20%60$%7Bpositions%5BnextIndex%5D.top%7Dpx%60;%20%20%20%20%20%20%20%20%20%20%20%20%20selectedElements%5Bi%5D.style.zIndex%20=%20%221000%22;%20%20%20%20%20%20%20%20%20%7D%20%20%20%20%20%7D%20%20%20%20%20%20%20%20%20%20document.addEventListener(%22click%22,%20swapElements);%20%20%20%20%20document.addEventListener(%22keydown%22,%20swapElements);%20%7D)();
